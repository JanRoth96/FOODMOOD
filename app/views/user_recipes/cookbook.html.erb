<% @user_recipes.each do |recipe| %>
  <div class="container d-flex">
    <div class="pic">
      <%= cl_image_tag recipe.recipe.photo.key,
                 :cloud_name => "diq0zfsun", class: "recipe-img" %>
    </div>
    <div class="detail">
      <%= recipe.recipe.name %>
    </div>
  </div>
<% end %>
