<div class="vertical">
  <div class="dish-container">
    <div class="dish">
      <%= cl_image_tag image,
                       :cloud_name => "dxpqczcfs", class:"dish-image"%>
      <div class="dish-description">
        <div class="dish-name">
          <span><%=name%></span>
        </div>
        <div class="dish-cuisine">
          <span><%=cuisine%></span>
        </div>
        <div class="dish-time">
          <span><%=time%> mins</span>
        </div>
      </div>
    </div>
  </div>
</div>

